[server]
port = 8080
enable_print_routes = false
read_timeout = "30s"
write_timeout = "30s"

[server.limiter]
max = 100
expiration = "1m"

[server.cross_origin_policy]
crop = "cross-origin"
cors_allow_origins = "*"
cors_allow_methods = "GET,POST,PUT,DELETE,HEAD,OPTIONS"
cors_allow_headers = "Origin,Content-Type,Accept,Authorization,Cache-Control,If-None-Match"

[images]
max_image_dimension = 8192
max_file_size_in_bytes= 52428800
source="local"

[images.awss3]
bucket=""

[images.local]
base_path = ""

[experimental]
enable_upload_api = false
